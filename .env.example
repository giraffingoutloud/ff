# Copy this file to .env.local and fill in your actual values
# Never commit .env or .env.local files to version control

# Yahoo Fantasy API credentials
VITE_YAHOO_CLIENT_ID=your_yahoo_client_id_here
VITE_YAHOO_CLIENT_SECRET=your_yahoo_client_secret_here

# ESPN API credentials (if needed)
VITE_ESPN_SWID=your_espn_swid_here
VITE_ESPN_S2=your_espn_s2_token_here

# Sleeper API (public API, no key needed)
VITE_SLEEPER_API_BASE=https://api.sleeper.app/v1

# Feature flags
VITE_ENABLE_REALTIME_UPDATES=true
VITE_ENABLE_DATA_VALIDATION=false