{
  "name": "fantasy-football-draft-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:engine": "vitest src/tests/evaluationEngine.test.ts",
    "test:regression": "vitest src/tests/evaluationEngine.test.ts --reporter=json --outputFile=test-results/regression.json",
    "analyze": "tsx scripts/run_full_analysis.ts",
    "analyze:regression": "tsx scripts/run_full_analysis.ts --regression",
    "calibrate": "tsx scripts/calibrate_weights.ts",
    "build:sos": "tsx scripts/build_sos.ts",
    "train:auction": "tsx scripts/fit_auction_model.ts",
    "validate": "tsx scripts/run_full_analysis.ts --validate-only",
    "benchmark": "tsx scripts/run_full_analysis.ts --benchmark",
    "audit:data": "tsx scripts/auditCanonicalData.ts"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.11",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.539.0",
    "node-fetch": "^3.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "framer-motion": "^12.23.12",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "recharts": "^3.1.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^2.1.9"
  }
}
